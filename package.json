{
  "name": "iobroker.innoxel",
  "version": "0.3.1",
  "description": "Adapter for Innoxel Master 3",
  "author": {
    "name": "matthsc",
    "email": "matthsc@gmx.net"
  },
  "homepage": "https://github.com/matthsc/ioBroker.innoxel",
  "license": "MIT",
  "keywords": [
    "innoxel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/ioBroker.innoxel"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.8",
    "innoxel-soap": "~0.4.0",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.9",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.21.5",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/adapter-react-v5": "^4.1.0",
    "@iobroker/testing": "^4.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@mui/styles": "^5.13.2",
    "@tsconfig/node18": "^2.0.1",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.18",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "run-p prebuild:*",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "translate": "translate-adapter",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "run-s test:ts test:package",
    "test-all": "run-s test:*",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "check-all": "run-s check lint test",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "release": "release-script -p iobroker license"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/matthsc/ioBroker.innoxel/issues"
  },
  "readmeFilename": "README.md"
}
