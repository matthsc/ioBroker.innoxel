{
  "name": "iobroker.innoxel",
  "version": "0.4.2",
  "description": "Adapter for Innoxel Master 3",
  "author": {
    "name": "matthsc",
    "email": "matthsc@gmx.net"
  },
  "homepage": "https://github.com/matthsc/ioBroker.innoxel",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "innoxel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/ioBroker.innoxel"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.1.6",
    "innoxel-soap": "~0.5.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.2",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@iobroker/adapter-dev": "^1.3.0",
    "@iobroker/adapter-react-v5": "^7.2.1",
    "@iobroker/testing": "^4.1.3",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/styles": "^5.16.7",
    "@tsconfig/node18": "^18.2.4",
    "@types/chai": "^4.3.17",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.7",
    "@types/node": "^18.19.44",
    "@types/proxyquire": "^1.3.31",
    "@types/react": "^17.0.80",
    "@types/react-dom": "^17.0.25",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^3.3.3",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.10",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "run-p prebuild:*",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "translate": "translate-adapter",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "run-s test:ts test:package",
    "test-all": "run-s test:*",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "check-all": "run-s check lint test",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "prettier": "prettier --write .",
    "release": "release-script -p iobroker license"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/matthsc/ioBroker.innoxel/issues"
  },
  "readmeFilename": "README.md"
}
